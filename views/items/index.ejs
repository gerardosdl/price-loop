<%- include('../partials/header.ejs') %>

<h1>All Items: </h1>

<% if (items.length) { %>
  <ul>
    <% items.forEach((item) => { %>
      <li>
        <a href="/items/<%= item._id %> ">
          <%= item.name %>
        </a>
      </li>
    <% }) %>
  </ul>
<% } else { %>
  <p>There Are No Items Yet</p>
<% } %>


<%- include('../partials/footer.ejs') %>